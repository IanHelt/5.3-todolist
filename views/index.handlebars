<form class="todo-form" action="/" method="post">
  <input type="text" name="todoInput" value="" placeholder="Add a todo..."/>
  <button type="submit" name="todoButton">Add Todo</button>
</form>
<ul class="generated-list">
{{#each todos}}
<form class="complete-form" action="/complete" method="post">
<li>
<input type="hidden" name="arrayKey" value="{{@key}}"/><input type="hidden" name="inputValue" value="{{todoObject}}"/>
<button type="submit">Mark as complete</button>{{todoObject}}
</li>
</form>
{{/each}}
</ul>

<ul class="complete-list">
  {{#each completed}}
  <li>
  {{inputValue}}
</li>
  {{/each}}
</ul>
